
public class tableconfirm {
	private String Note, CustomerAddress, CustomerPhone, CustomerName, RestaurantAddress, RestaurantName;
	int BillID,OrderID;
	public String getNote() {
		return Note;
	}
	
	public String getCustomerAddress() {
		return CustomerAddress;
	}
	
	public String getCustomerPhone() {
		return CustomerPhone;
	}
	
	public String getCustomerName() {
		return CustomerName;
	}
	
	public String getRestaurantAddress() {
		return RestaurantAddress;
	}
	
	public String getRestaurantName() {
		return RestaurantName;
	}
	
	public int getBillID() {
		return BillID;
	}
	
	public int getOrderID() {
		return OrderID;
	}
	
	public tableconfirm(String note, String customerAddress, String customerPhone, String customerName,
			String restaurantAddress, String restaurantName, int billID, int orderID) {
		Note = note;
		CustomerAddress = customerAddress;
		CustomerPhone = customerPhone;
		CustomerName = customerName;
		RestaurantAddress = restaurantAddress;
		RestaurantName = restaurantName;
		BillID = billID;
		OrderID = orderID;
	}
	
}
